"""

Permutations & Combinations

"""

import itertools

# String
a = 'ABCD'

# Generating permutations
print("Permutations:")
for perm in itertools.permutations(a):
    print(''.join(perm))

# Generating combinations
print("\nCombinations:")
for r in range(1, len(a) + 1):
    for comb in itertools.combinations(a, r):
        print(''.join(comb))
